{
  "name": "@nathanjhood/esbuild-scripts",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/nathanjhood/esbuild-scripts",
  "repository": {
    "type": "git",
    "url": "git@github.com/nathanjhood/nathanjhood.github.io.git",
    "directory": "packages/@nathanjhood/esbuild-scripts"
  },
  "scripts": {
    "test": "tsx test/run.ts",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "build": "pkgroll --build",
    "watch": "pkgroll --watch",
    "minify": "pkgroll --minify",
    "sourcemap": "pkgroll --sourcemap",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./prettier.config.mjs",
    "format:fix": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./prettier.config.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "bin": {
    "parseCwd": "./dist/process/parseCwd.js",
    "parseCommand": "./dist/process/parseCommand.js",
    "parseArgV": "./dist/process/parseArgv.js",
    "parseEnv": "./dist/process/parseEnv.js",
    "build": "./dist/scripts/build.js",
    "esbuild-scripts": "./dist/cli.js"
  },
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=20"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "browserslist": "^4.23.3",
    "esbuild": "0.24.0",
    "ms": "^2.1.3",
    "pkgroll": "2.5.0",
    "tsx": "4.19.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/node": "22.5.5",
    "cli-testing-library": "^2.0.2",
    "debug": "^4.3.7",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "9.1.0",
    "globals": "^15.9.0",
    "prettier": "3.3.3",
    "tsc": "2.0.4",
    "tslib": "2.7.0",
    "typescript": "5.6.2",
    "typescript-eslint": "^8.5.0"
  }
}
